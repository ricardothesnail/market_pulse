# Environment Variables Template
# Copy this to .env.local and fill in your API keys

# Stock & Fundamentals Data (REQUIRED)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Financial News (Optional but recommended)
NEXT_PUBLIC_NEWS_API_KEY=your_newsapi_key_here

# Vercel Deployment (Only needed for GitHub Actions auto-deploy)
VERCEL_TOKEN=your_vercel_token_here

# Optional: Custom preferences
STOCKS_UPDATE_FREQUENCY=daily
MEMECOIN_COUNT=5
MARKET_OPEN_TIME=09:00:00
MARKET_TIMEZONE=America/New_York
